<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.heed">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fin"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar"
        tools:ignore="GoogleAppIndexingWarning">


        <activity android:name=".Savings" />
        <activity android:name=".Workers" />
        <activity android:name=".Teachers" />
        <activity android:name=".Students" />
        <activity android:name=".Parenting" />
        <activity android:name=".FoodHabitsActivity" />
        <activity android:name=".GeneralActivity" />
        <activity android:name=".Dashboard" />
        <activity android:name=".OnBoarding" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>